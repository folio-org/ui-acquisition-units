{
  "name": "@folio/acquisition-units",
  "version": "2.2.0",
  "description": "Acquisition units",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/folio-org/ui-acquisition-units.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "test": "stripes test karma",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@bigtest/interactor": "^0.9.3",
    "@bigtest/mocha": "^0.5.1",
    "@bigtest/react": "^0.1.2",
    "@folio/eslint-config-stripes": "^5.0.0",
    "@folio/stripes": "^5.0.0",
    "@folio/stripes-cli": "^1.12.0",
    "babel-eslint": "^10.0.0",
    "chai": "^4.2.0",
    "core-js": "^3.6.1",
    "eslint": "^7.7.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-jest": "^23.0.4",
    "faker": "^5.1.0",
    "miragejs": "^0.1.32",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intl": "^5.7.1",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.3",
    "sinon": "^7.0.0"
  },
  "dependencies": {
    "@folio/stripes-acq-components": "^2.2.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.0",
    "query-string": "^6.7.0",
    "react-redux": "^5.1.1",
    "react-router-prop-types": "^1.0.4",
    "redux": "^3.7.2",
    "redux-form": "^7.4.2"
  },
  "optionalDependencies": {
    "@folio/plugin-find-user": "*"
  },
  "peerDependencies": {
    "@folio/stripes": "^5.0.0",
    "react": "*",
    "react-intl": "^5.7.1",
    "react-router-dom": "^5.2.0"
  },
  "stripes": {
    "actsAs": ["settings"],
    "displayName": "ui-acquisition-units.meta.title",
    "queryResource": "query",
    "route": "/acquisition-units",
    "okapiInterfaces": {
      "acquisitions-units": "1.1",
      "users": "15.1"
    },
    "stripesDeps": [
      "@folio/stripes-acq-components"
    ],
    "permissionSets": [
      {
        "permissionName": "settings.acquisition-units.enabled",
        "displayName": "Settings (acquisition units): display list of settings pages",
        "subPermissions": [
          "settings.enabled",
          "users.collection.get",
          "usergroups.collection.get",
          "acquisitions-units.units.all",
          "acquisitions-units.memberships.all"
        ],
        "visible": false
      },
      {
        "permissionName": "ui-acquisition-units.settings.all",
        "displayName": "Settings (acquisition units): Can view, edit, create and delete acquisition units",
        "subPermissions": [
          "settings.acquisition-units.enabled"
        ],
        "visible": true
      }
    ]
  }
}
