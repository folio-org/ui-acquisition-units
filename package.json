{
  "name": "@folio/acquisition-units",
  "version": "1.0.0",
  "description": "Acquisition units",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/folio-org/ui-acquisition-units.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "test": "stripes test karma",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@bigtest/interactor": "^0.8.1",
    "@bigtest/mirage": "^0.0.1",
    "@bigtest/mocha": "^0.5.1",
    "@bigtest/react": "^0.1.2",
    "@folio/eslint-config-stripes": "^4.1.0",
    "@folio/stripes": "^2.7.0",
    "@folio/stripes-cli": "^1.12.0",
    "babel-eslint": "^9.0.0",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^5.6.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-no-only-tests": "^2.3.0",
    "mocha": "^5.2.0",
    "sinon": "^6.3.4"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "prop-types": "^15.6.0",
    "query-string": "^6.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.5.1",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.1.1",
    "react-router-prop-types": "^1.0.4",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2"
  },
  "optionalDependencies": {
    "@folio/plugin-find-user": "^1.7.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^2.7.0",
    "react": "*",
    "react-dom": "*",
    "redux": "*",
    "redux-form": "*"
  },
  "stripes": {
    "type": "settings",
    "displayName": "ui-acquisition-units.meta.title",
    "queryResource": "query",
    "route": "/acquisition-units",
    "okapiInterfaces": {
      "acquisitions-units": "1.0",
      "users": "15.0"
    },
    "permissionSets": [
      {
        "permissionName": "settings.acquisition-units.enabled",
        "displayName": "Settings (acquisition units): display list of settings pages",
        "subPermissions": [
          "settings.enabled",
          "users.collection.get",
          "usergroups.collection.get",
          "acquisitions-units.units.all",
          "acquisitions-units.memberships.all"
        ],
        "visible": true
      }
    ]
  }
}
